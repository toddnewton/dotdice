
define example
  var modifier
  var dice

  make (mod dice)
    this.modifier = mod
    this.dice = dice
  end make

  roll ()
    return d + modifier
  end roll

  setmod (x)
    this.modifier = x
  end setmod

  setdice (dice)
    this.dice = dice
  end setdice
end define

f (x)
